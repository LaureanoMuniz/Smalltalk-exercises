!methodRemoval: Entero #restarFraccion: stamp: 'LM 4/22/2022 12:09:30'!
restarFraccion: aSubtrahend 
	
	^ self * aSubtrahend denominator - aSubtrahend numerator / aSubtrahend denominator.
	!
!Entero methodsFor: 'arithmetic operations' stamp: 'LM 4/22/2022 12:09:58' prior: 50486951!
restarAEntero: enteroASerRestado
	^ self class with:  enteroASerRestado integerValue - value.! !
!Entero methodsFor: 'arithmetic operations' stamp: 'LM 4/22/2022 12:10:17' prior: 50486957!
restarAFraccion: fraccionASerRestada
	^ fraccionASerRestada numerator - (self * fraccionASerRestada denominator) /  fraccionASerRestada denominator.! !
!Entero methodsFor: 'arithmetic operations' stamp: 'LM 4/22/2022 12:10:46' prior: 50486971!
sumarEntero: enteroASerSumado
	
	^ self class with: value + enteroASerSumado integerValue.
	
! !
!Entero methodsFor: 'arithmetic operations' stamp: 'LM 4/22/2022 12:11:00' prior: 50486976!
sumarFraccion: fraccionASerSumada
	
	
	^ (self * (fraccionASerSumada denominator) + fraccionASerSumada numerator) / fraccionASerSumada denominator.
! !
!Fraccion methodsFor: 'arithmetic operations' stamp: 'LM 4/22/2022 12:11:28' prior: 50487095!
restarAEntero: enteroARestar 
	
	 ^  (enteroARestar * denominator) - numerator  / denominator.
 
! !
!Fraccion methodsFor: 'arithmetic operations' stamp: 'LM 4/22/2022 12:11:48' prior: 50487384!
restarAEntero: enteroASerRestado 
	
	 ^  (enteroASerRestado * denominator) - numerator  / denominator.
 
! !
!Fraccion methodsFor: 'arithmetic operations' stamp: 'LM 4/22/2022 12:12:59' prior: 50487101!
restarAFraccion: fraccionASerRestada 
	
	| newNumerator newDenominator |
	
	
	newNumerator := (denominator * fraccionASerRestada numerator) - (numerator * fraccionASerRestada denominator).
	newDenominator := denominator * fraccionASerRestada denominator.
	
	^newNumerator / newDenominator 
! !
!Fraccion methodsFor: 'arithmetic operations' stamp: 'LM 4/22/2022 12:13:21' prior: 50487113!
sumarEntero: enteroASerSumado

	^(enteroASerSumado * self denominator + self numerator) / self denominator .
! !
!Fraccion methodsFor: 'arithmetic operations' stamp: 'LM 4/22/2022 12:13:42' prior: 50487119!
sumarFraccion: fraccionASerSumada 
	
	| newNumerator newDenominator |
	
	
	newNumerator := (numerator * fraccionASerSumada denominator) + (denominator * fraccionASerSumada numerator).
	newDenominator := denominator * fraccionASerSumada denominator.
	
	^newNumerator / newDenominator ! !

