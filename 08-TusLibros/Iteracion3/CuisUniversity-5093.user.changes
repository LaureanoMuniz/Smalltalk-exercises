!SentenceFinderByPrefix methodsFor: 'as yet unclassified' stamp: 'LM 4/28/2022 12:53:19' prior: 50505962!
findSentencesWithPrefix: aPrefix inOOStack: anOOStack
	
	
	
	| currentHolder sentencesThatBeginsWithPrefix |
	
	(self PrefixIsValid: aPrefix) ifFalse: [^self error: self class InvalidPrefix].
	
	sentencesThatBeginsWithPrefix := OrderedCollection new.
	
	currentHolder := anOOStack headElementHolder .
	
		[currentHolder hasHolderUnderneath] whileTrue: [
		
		
		(currentHolder elementOnTopOfMe beginsWith: aPrefix) 
		ifTrue: 
			[sentencesThatBeginsWithPrefix add: currentHolder elementOnTopOfMe].
	
		currentHolder := currentHolder holderOnBottomOfMe.
	].

	^sentencesThatBeginsWithPrefix.! !
